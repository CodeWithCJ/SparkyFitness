# Supabase Project URL - Found in Supabase Project Settings -> API
SUPABASE_URL="YOUR_SUPABASE_URL_HERE"

# Supabase Service Role Key - Found in Supabase Project Settings -> API (keep this secret!)
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY_HERE"

# Port for the Sparky Fitness Server to listen on
SPARKY_FITNESS_SERVER_PORT=3010

# Logging level for the server. Options: DEBUG, INFO, WARN, ERROR, SILENT
LOG_LEVEL=INFO