{
  "expo": {
    "name": "SparkyFitnessMobile",
    "slug": "sparkyfitnessmobile",
    "version": "1.0.0",
    "scheme": "sparkyfitnessmobile",
    "orientation": "portrait",
    "icon": "./assets/icons/appstore.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "plugins": [
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "This app requires HealthKit access to read and write health data.",
          "NSHealthUpdateUsageDescription": "This app requires HealthKit access to read and write health data.",
          "background": true
        }
      ],
      "react-native-background-fetch",
      "expo-health-connect",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ],
      "react-native-health-connect",
      "expo-web-browser",
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "498a86c5-344f-4d2c-9033-dfd720e4a383"
      },
      "APP_VARIANT": "production"
    }
  }
}
